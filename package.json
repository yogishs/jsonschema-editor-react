{
	"name": "@yogishs/json-schema-editor",
	"version": "2.2.0",
	"description": "JsonSchema Editor React Control",
	"repository": "https://github.com/optum/jsonschema-editor-react",
	"license": "Apache 2.0",
	"engines": {
		"node": ">=10.18.0 <11 || >=12.14.0 <13 || >=13.5.0"
	},
	"source": "src/index.ts",
	"main": "dist/index.js",
	"files": [
		"dist"
	],
	"keywords": [
		"react"
	],
	"scripts": {
		"start": "react-scripts start",
		"build": "microbundle --jsx React.createElement",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"storybook": "rimraf docs && start-storybook -p 6006 -s docs",
		"build-storybook": "rimraf docs && build-storybook --docs -o docs"
	},
	"eslintConfig": {
		"extends": "react-app",
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@hookstate/core": "^3.0.7",
		"@arco-design/web-react": "^2.45.0",
		"@arco-themes/react-yogishs": "^0.0.2",
		"framer-motion": "^10.0.0",
		"ramda": "^0.28.0",
		"react-icons": "^3.0.0",
		"typescript": "^4.9.5",
		"use-debounce": "^9.0.3",
		"web-vitals": "^3.1.1"
	},
	"peerDependencies": {
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-console": "^1.2.3",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/node-logger": "^6.5.16",
		"@storybook/preset-create-react-app": "^3.1.7",
		"@storybook/react": "^6.5.16",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^12.0.2",
		"@types/jest": "^29.4.0",
		"@types/node": "^12.0.0",
		"@types/ramda": "^0.28.23",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"microbundle": "^0.13.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "4.0.3"
	}
}
